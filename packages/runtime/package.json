{
  "name": "@renderify/runtime",
  "version": "0.0.1",
  "main": "dist/runtime.cjs.js",
  "module": "dist/runtime.esm.js",
  "umd:main": "dist/runtime.umd.min.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/runtime.cjs.mjs",
        "default": "./dist/runtime.cjs.js"
      },
      "module": "./dist/runtime.esm.js",
      "import": "./dist/runtime.cjs.mjs",
      "default": "./dist/runtime.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "preconstruct": {
    "umdName": "RenderifyRuntime",
    "entrypoints": [
      "index.ts"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc -p tsconfig.json --pretty false --noEmit",
    "lint": "biome lint src",
    "clean": "rm -rf dist dist-types"
  },
  "author": "unadlib",
  "license": "MIT",
  "description": "A runtime for Renderify",
  "dependencies": {
    "@renderify/ir": "workspace:^",
    "preact": "^10.28.3"
  }
}

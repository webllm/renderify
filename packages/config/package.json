{
  "name": "@renderify/config",
  "version": "0.0.1",
  "main": "dist/config.cjs.js",
  "module": "dist/config.esm.js",
  "umd:main": "dist/config.umd.min.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/config.cjs.mjs",
        "default": "./dist/config.cjs.js"
      },
      "module": "./dist/config.esm.js",
      "import": "./dist/config.cjs.mjs",
      "default": "./dist/config.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "preconstruct": {
    "umdName": "RenderifyConfig",
    "entrypoints": [
      "index.ts"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc -p tsconfig.json --pretty false --noEmit",
    "lint": "biome lint src",
    "clean": "rm -rf dist dist-types"
  },
  "author": "unadlib",
  "license": "MIT",
  "description": "A configuration module for Renderify"
}

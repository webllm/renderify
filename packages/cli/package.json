{
  "name": "@renderify/cli",
  "version": "0.0.1",
  "main": "dist/cli.cjs.js",
  "module": "dist/cli.esm.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/cli.cjs.mjs",
        "default": "./dist/cli.cjs.js"
      },
      "module": "./dist/cli.esm.js",
      "import": "./dist/cli.cjs.mjs",
      "default": "./dist/cli.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "bin"
  ],
  "preconstruct": {
    "entrypoints": [
      "index.ts"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc -p tsconfig.json --pretty false --noEmit",
    "lint": "biome lint src",
    "clean": "rm -rf dist dist-types"
  },
  "author": "unadlib",
  "license": "MIT",
  "description": "A CLI for Renderify",
  "dependencies": {
    "@renderify/core": "workspace:^",
    "@renderify/ir": "workspace:^",
    "@renderify/llm-openai": "workspace:^",
    "@renderify/runtime": "workspace:^"
  },
  "bin": {
    "renderify": "bin/renderify.js"
  }
}

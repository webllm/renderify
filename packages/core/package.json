{
  "name": "@renderify/core",
  "version": "0.0.1",
  "main": "dist/core.cjs.js",
  "types": "dist/core.cjs.d.ts",
  "module": "dist/core.esm.js",
  "umd:main": "dist/core.umd.min.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/core.cjs.mjs",
        "default": "./dist/core.cjs.js"
      },
      "module": "./dist/core.esm.js",
      "import": "./dist/core.cjs.mjs",
      "default": "./dist/core.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "preconstruct": {
    "umdName": "RenderifyCore",
    "entrypoints": [
      "index.ts"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc -p tsconfig.json --pretty false --noEmit",
    "lint": "biome lint src",
    "clean": "rm -rf dist dist-types"
  },
  "author": "unadlib",
  "license": "MIT",
  "description": "The core module for Renderify",
  "keywords": [
    "renderify",
    "llm",
    "runtime",
    "codegen",
    "ui-runtime"
  ],
  "homepage": "https://github.com/webllm/renderify#readme",
  "bugs": {
    "url": "https://github.com/webllm/renderify/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webllm/renderify.git",
    "directory": "packages/core"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "dependencies": {
    "@renderify/ir": "workspace:^",
    "@renderify/security": "workspace:^",
    "@renderify/runtime": "workspace:^",
    "preact": "^10.28.3",
    "preact-render-to-string": "^6.6.5"
  }
}

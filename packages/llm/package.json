{
  "name": "@renderify/llm",
  "version": "0.0.1",
  "description": "LLM providers for Renderify",
  "main": "dist/llm.cjs.js",
  "module": "dist/llm.esm.js",
  "umd:main": "dist/llm.umd.min.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/llm.cjs.mjs",
        "default": "./dist/llm.cjs.js"
      },
      "module": "./dist/llm.esm.js",
      "import": "./dist/llm.cjs.mjs",
      "default": "./dist/llm.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "preconstruct": {
    "umdName": "RenderifyLLM",
    "entrypoints": [
      "index.ts"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc -p tsconfig.json --pretty false --noEmit",
    "lint": "biome lint src",
    "clean": "rm -rf dist dist-types"
  },
  "author": "unadlib",
  "license": "MIT",
  "dependencies": {
    "@renderify/core": "workspace:^",
    "@renderify/ir": "workspace:^"
  }
}

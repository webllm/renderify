{
  "name": "@renderify/security",
  "version": "0.4.0",
  "main": "dist/security.cjs.js",
  "types": "dist/security.d.ts",
  "module": "dist/security.esm.js",
  "exports": {
    ".": {
      "types": "./dist/security.d.ts",
      "import": "./dist/security.esm.js",
      "require": "./dist/security.cjs.js",
      "default": "./dist/security.esm.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc -p tsconfig.json --pretty false --noEmit",
    "lint": "biome lint src",
    "clean": "rm -rf dist dist-types",
    "build:repo": "tsup --config tsup.config.ts",
    "watch:repo": "tsup --config tsup.config.ts --watch"
  },
  "author": "unadlib",
  "license": "MIT",
  "description": "Security policy engine for Renderify",
  "keywords": [
    "renderify",
    "security",
    "policy",
    "runtime",
    "sandbox",
    "llm"
  ],
  "homepage": "https://github.com/webllm/renderify#readme",
  "bugs": {
    "url": "https://github.com/webllm/renderify/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webllm/renderify.git",
    "directory": "packages/security"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "dependencies": {
    "@renderify/ir": "workspace:^"
  }
}

{
  "name": "@renderify/security",
  "version": "0.0.1",
  "main": "dist/security.cjs.js",
  "types": "dist/security.cjs.d.ts",
  "module": "dist/security.esm.js",
  "umd:main": "dist/security.umd.min.js",
  "exports": {
    ".": {
      "types": {
        "import": "./dist/security.cjs.mjs",
        "default": "./dist/security.cjs.js"
      },
      "module": "./dist/security.esm.js",
      "import": "./dist/security.cjs.mjs",
      "default": "./dist/security.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "preconstruct": {
    "umdName": "RenderifySecurity",
    "entrypoints": [
      "index.ts"
    ]
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "typecheck": "tsc -p tsconfig.json --pretty false --noEmit",
    "lint": "biome lint src",
    "clean": "rm -rf dist dist-types"
  },
  "author": "unadlib",
  "license": "MIT",
  "description": "Security policy engine for Renderify",
  "keywords": [
    "renderify",
    "security",
    "policy",
    "runtime",
    "sandbox",
    "llm"
  ],
  "homepage": "https://github.com/webllm/renderify#readme",
  "bugs": {
    "url": "https://github.com/webllm/renderify/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/webllm/renderify.git",
    "directory": "packages/security"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "sideEffects": false,
  "dependencies": {
    "@renderify/ir": "workspace:^"
  }
}
